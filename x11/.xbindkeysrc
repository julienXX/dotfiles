#b-
"brightnessctl set 5%+"
    m:0x0 + c:233
    XF86MonBrightnessUp

#b+
"brightnessctl set 5%-"
    m:0x0 + c:232
    XF86MonBrightnessDown

#vm
"amixer sset Master toggle"
    m:0x0 + c:121
    XF86AudioMute

#v+
"amixer sset Master 5+ unmute"
    m:0x0 + c:123
    XF86AudioRaiseVolume

#v-
"amixer sset Master 5- unmute"
    m:0x0 + c:122
    XF86AudioLowerVolume

"xset s activate"
    Insert

"jumpapp emacs"
    Mod4 + 1

"jumpapp alacritty"
    Mod4 + 2

"jumpapp firefox"
    Mod4 + 3

"jumpapp slack"
    Mod4 + 4

"jumpapp thunderbird"
    Mod4 + 5

"jumpapp chromium"
    Mod4 + 6

"jumpapp hexchat"
    Mod4 + 7
